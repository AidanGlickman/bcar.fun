{
  "name": "BCAr",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An extension to override your new tab page with bcar.fun",
  "homepage_url": "https://bcar.fun",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  }
}